    {
      "parameters": {
        "jsCode": "// Input is merged, so use current item JSON\nconst mobileData = $json.mobile || $json;   // support merged result\nconst desktopData = $json.desktop || $json;\n\nconst emailRecipients = $('Edit Fields').first().json.email_recipients;\n\n// If API returned an error, store that information\nif (mobileData.error || desktopData.error) {\n  return {\n    json: {\n      timestamp: new Date().toISOString(),\n      url: $('Edit Fields').first().json.URL,\n\n      email_recipients: emailRecipients,\n\n      error_mobile: mobileData.error || null,\n      error_desktop: desktopData.error || null,\n      status: \"API_ERROR\"\n    }\n  };\n}\n\n// Helper: Extract numeric metric\nfunction getMetric(data, metricName) {\n  return data?.lighthouseResult?.audits?.[metricName]?.numericValue || null;\n}\n\n// Extract metrics\nconst mobileLCP = getMetric(mobileData, 'largest-contentful-paint');\nconst mobileINP = getMetric(mobileData, 'interaction-to-next-paint') || getMetric(mobileData, 'max-potential-fid');\nconst mobileCLS = getMetric(mobileData, 'cumulative-layout-shift');\nconst mobileScore = mobileData?.lighthouseResult?.categories?.performance?.score || null;\n\nconst desktopLCP = getMetric(desktopData, 'largest-contentful-paint');\nconst desktopINP = getMetric(desktopData, 'interaction-to-next-paint') || getMetric(desktopData, 'max-potential-fid');\nconst desktopCLS = getMetric(desktopData, 'cumulative-layout-shift');\nconst desktopScore = desktopData?.lighthouseResult?.categories?.performance?.score || null;\n\n// Return final structured output\nreturn {\n  json: {\n    timestamp: new Date().toISOString(),\n    url: $('Edit Fields').first().json.URL,\n\n    // âœ… Now email recipients is included\n    email_recipients: emailRecipients,\n\n    mobile_lcp: mobileLCP,\n    mobile_inp: mobileINP,\n    mobile_cls: mobileCLS,\n    mobile_score: mobileScore ? Math.round(mobileScore * 100) : null,\n\n    desktop_lcp: desktopLCP,\n    desktop_inp: desktopINP,\n    desktop_cls: desktopCLS,\n    desktop_score: desktopScore ? Math.round(desktopScore * 100) : null,\n\n    status: \"SUCCESS\"\n  }\n};\n"
      },
